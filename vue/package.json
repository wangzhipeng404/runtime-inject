{
  "name": "vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --no-clean",
    "lint": "vue-cli-service lint",
    "dev:shell": "vue-cli-service serve ./src/components/Shell/shell.js",
    "dev:lib": "vue-cli-service serve ./dev.js",
    "dev:page": "vue-cli-service serve ./dev.js",
    "lib:shell": "vue-cli-service build --target lib --dest dist/shell --name shell ./src/components/Shell/Shell.vue",
    "lib:hello": "vue-cli-service build --target lib --formats umd-min --dest dist/h5-components/hello --name hello ./packages/hello/hello.vue"
  },
  "dependencies": {
    "@babel/standalone": "^7.15.8",
    "@vue/babel-sugar-v-model": "^1.2.3",
    "axios": "^0.24.0",
    "babel-plugin-jsx-v-model": "^2.0.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.7",
    "echarts": "^5.2.2",
    "stylus": "^0.58.1",
    "vant": "^2.12.30",
    "vconsole": "^3.9.4",
    "vue": "^2.6.11",
    "vue-axios": "^3.3.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
